"use strict";function refreshList(){var e,t="/"+scriptData.path,i=!0;null===scriptData.appendField||void 0!==(e=$("#filter-"+scriptData.appendField).val())&&""!==e&&(t+="/"+e),["brand","max-price","screen-size","screen-size-range","screen-resolution","screen-refresh-rate","max-screen-response-time","cpu","cpu-chipmaker","gpu","gpu-chipmaker","memory","max-weight","max-rating","cores"].forEach(function(e){var a;null!==scriptData.appendField&&e===scriptData.appendField||void 0!==(a=$("#filter-"+e).val())&&""!==a&&(t+=(i?"?":"&")+e+"="+a,i=!1)}),scriptData.games&&0<scriptData.games.length&&(t+=(i?"?":"&")+"games="+scriptData.games.join("|")),window.location.href=t}$('#compare .filter select, #compare .filter input[type="hidden"]').change(refreshList),$("#filter-cpu-chipmaker-choice a").click(function(e){e=$(e.target).attr("data-chipmaker");$("#filter-cpu-chipmaker").val(e).change()}),$("#filter-gpu-chipmaker-choice a").click(function(e){e=$(e.target).attr("data-chipmaker");$("#filter-gpu-chipmaker").val(e).change()}),scriptData.games&&$("#filter-games").typeahead({minLength:0,highlight:!1,autoselect:!0},{name:"games",display:"name",source:gamesBloodhound,templates:{suggestion:function(e){return"<div>"+e.name+"</div>"}},limit:10}).bind("typeahead:select",function(e,a){scriptData.games.includes(a.slug)||(scriptData.games.push(a.slug),refreshList()),$(e.currentTarget).typeahead("close").typeahead("val","").blur()});